{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "vendor": { "type": "string" },
            "image_id": { "type": "string" },
            "created_at": { "type": "string" }, 
            "moderation_labels": { "type": "array" }, 
            "labels": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "Name": { "type": "string" },
                        "Confidence": { "type": "number" } 
                    },
                    "required": ["Name", "Confidence"],
                    "additionalProperties": true 
                }
            }
        },
        "required": [
            "labels",
            "vendor",
            "image_id",
            "created_at"
        ],
        
        "additionalProperties": true 
    }
}